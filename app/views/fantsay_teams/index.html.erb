<% title "Fantsay Teams" %>

<table>
  <tr>
    <th>Name</th>
    <th>League</th>
    <th>User</th>
  </tr>
  <% for fantsay_team in @fantsay_teams %>
    <tr>
      <td><%=h fantsay_team.name %></td>
      <td><%=h fantsay_team.league_id %></td>
      <td><%=h fantsay_team.user_id %></td>
      <td><%= link_to "Show", fantsay_team %></td>
      <td><%= link_to "Edit", edit_fantsay_team_path(fantsay_team) %></td>
      <td><%= link_to "Destroy", fantsay_team, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Fantsay Team", new_fantsay_team_path %></p>
